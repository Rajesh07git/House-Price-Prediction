{
    "version": 2,
    "builds": [
        { "src": "server/index.py", "use": "@vercel/python" }
    ],
    "routes": [
        { "src": "/(.*)", "dest": "server/index.py" }
    ],
    "functions": {
      "api/**": {
        "excludeFiles": "{.next,*.cache,node_modules,public,app}/**"
      }
    }
}

  