{
    "functions": {
    "server/api/*.js": {
      "runtime": "nodejs18.x"
    }
    },
    "builds": [
        {"src": "/server/index.py", "use": "@vercel/python"},
        { "src": "server/api/*.js", "use": "@vercel/node" }
    ],
    "routes": [
        {"src": "/(.*)", "dest": "/server/index.py"}
    ]
}