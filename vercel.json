{
    "version": 2,
    "builds": [
        { "src": "server/index.py", "use": "@vercel/python" },
        { "src": "server/util.py", "use": "@vercel/python" }
    ],
    "routes": [
        { "src": "/(.*)", "dest": "server/index.py" },
        { "src": "/(.*)", "dest": "server/util.py" }
    ]
    
}

  